int main(void) {

	int a,b,resultado;

	a = ingresoNumeroPositivo();
	b = ingresoNumeroPositivo();

	resultado = suma(a, b);

	printf("\nLA SUMA ES -> %d\n",resultado);



	system("pause");
	return EXIT_SUCCESS;
}
